@model NewBrainfieldNetCore.Viewmodels.ListStudyVideosViewModel

@{
    Layout = "~/Views/Shared/_FrontendLayout.cshtml";
}

<div class="page-title">
    <div class="d-table">
        <div class="d-table-cell">
            <div class="container">
                <h3>Free Videos</h3>
            </div>
        </div>
    </div>
</div>

<section class="courses-area ptb-100">
    <div class="container">
        <div class="row">
            @foreach (var x in Model.Videos)
            {
                <div class="col-lg-6 col-md-6">
                    <div class="courses-item">
                        <div class="courses-img">
                            @Html.Raw(x.VideoURL)
                        </div>
                        <div class="courses-content">
                            <h3>
                                @x.Title
                            </h3>
                        </div>
                    </div>
                </div>
            }
        </div>
    </div>
</section>